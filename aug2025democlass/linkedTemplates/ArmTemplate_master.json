					"ls_sqlservertoadlsgen2_properties_typeProperties_database": {
						"value": "[parameters('ls_sqlservertoadlsgen2_properties_typeProperties_database')]"
					},
					"ls_sqlservertoadlsgen2_properties_typeProperties_userName": {
						"value": "[parameters('ls_sqlservertoadlsgen2_properties_typeProperties_userName')]"
					}
